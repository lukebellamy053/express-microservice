{
  "name": "node-express-microservice",
  "version": "0.5.3",
  "description": "Creates a new simple microservice server using express and nodejs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukebellamy053/express-microservice"
  },
  "types": "src/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register **/*.spec.ts --reporter mocha-multi-reporters --reporter-options configFile=.mocha/config.json",
    "test:coverage": "nyc npm test",
    "prepare": "tsc",
    "update:minor": "call grunt bump && tsc --skipLibCheck & exit 0",
    "clean": "git clean -fx && git clean -fd"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.12"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/chai": "^4.1.6",
    "@types/es6-shim": "^0.31.37",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.0.18",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "grunt": "^1.0.3",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.3.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^3.1.2",
    "nyc": "^13.1.0",
    "sinon": "^7.3.1",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "author": "Luke Bellamy",
  "license": "MIT Licence"
}
