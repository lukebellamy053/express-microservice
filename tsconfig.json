{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "**/*.d.ts"
   ]
}